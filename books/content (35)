<td data-index="7819"></td>
<td data-index="7820"><span class="center" data-index="7821">(1.05)<sup data-index="7822">2</sup></span></td>
<td data-index="7823"></td>
<td data-index="7824"><span class="center" data-index="7825"><span class="red" data-index="7826">.90703</span></span></td>
<td data-index="7827"><span class="center" data-index="7828">1/(1.05)<sup data-index="7829">2</sup></span></td>
</tr>
<tr class="bdr1" data-index="7830">
<td data-index="7831"></td>
<td data-index="7832"><span class="center" data-index="7833">3</span></td>
<td data-index="7834"><span class="center" data-index="7835">1.00000</span></td>
<td data-index="7836"></td>
